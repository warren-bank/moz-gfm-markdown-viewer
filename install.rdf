<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	<Description about="urn:mozilla:install-manifest">
		<em:id>moz-gfm-markdown-viewer@warren-bank.github.com</em:id>
		<em:version>1.0</em:version>
		<em:type>2</em:type>

		<em:name>GitHub Flavored Markdown (GFM) Viewer</em:name>
		<em:description>Dynamically renders pages written in raw markdown (.md) into HTML. Supports the variation of markdown syntax used on GitHub.</em:description>
		<em:creator>Warren Bank</em:creator>
		<em:homepageURL>https://github.com/warren-bank/moz-gfm-markdown-viewer</em:homepageURL>
		<em:optionsType>1</em:optionsType>
		<em:optionsURL>chrome://gfm-markdown-viewer/content/preferences_window.xul</em:optionsURL>
		<em:iconURL>chrome://gfm-markdown-viewer/skin/logo-32x32.png</em:iconURL>
		<em:icon64URL>chrome://gfm-markdown-viewer/skin/logo-64x64.png</em:icon64URL>

		<!-- Firefox -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>4.0</em:minVersion>
				<em:maxVersion>31.*</em:maxVersion>
			</Description>
		</em:targetApplication>

		<em:localized>
			<Description>
				<em:locale>en-US</em:locale>
				<em:name>GitHub Flavored Markdown (GFM) Viewer</em:name>
				<em:description>Dynamically renders pages written in raw markdown (.md) into HTML. Supports the variation of markdown syntax used on GitHub.</em:description>
				<em:creator>Warren Bank</em:creator>
			</Description>
		</em:localized>

	</Description>
</RDF>
